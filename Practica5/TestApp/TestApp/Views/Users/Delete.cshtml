@model TestApp.Library.DAL.Models.Users


    <form asp-action="Delete" method="post">
        <input type="hidden" asp-for="user_id" />
        <dl>
            <dt>First Name:</dt>
            <dd>@Model.first_names</dd>

            <dt>Last Name:</dt>
            <dd>@Model.last_names</dd>

            <dt>Email:</dt>
            <dd>@Model.email</dd>
        </dl>
        <a asp-action="Index" class="btn btn-dark">Cancel</a>
        <input type="submit" class="btn btn-danger" value="Delete" />

    </form>

