@model TestApp.Library.DAL.Models.Persons

<h1>Add Person</h1>



<form asp-action="Add" method="post">

    <input asp-for="first_names" type="text" placeholder="First Name" />
    <input asp-for="last_names" type="text" placeholder="Last Name" />
    <input asp-for="email" type="email" placeholder="Email" />

    <input type="submit" class="btn btn-success" value="Save" />
</form>

<a asp-action="Index">Back to list</a>